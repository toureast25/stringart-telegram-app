{
  "name": "StringArt Generator",
  "short_name": "StringArt",
  "description": "Create StringArt templates from photos with advanced color analysis",
  "version": "1.0.0",
  "author": "Tour_east",
  "telegram_mini_app": {
    "bot_username": "@stringart_generator_bot",
    "start_param": "stringart",
    "web_app_url": "https://toureast25.github.io/stringart-telegram-app/"
  },
  "icons": [
    {
      "src": "./i.webp",
      "sizes": "192x192",
      "type": "image/webp"
    }
  ],
  "theme_color": "#2ea6ff",
  "background_color": "#0f1115",
  "display": "standalone",
  "orientation": "portrait",
  "scope": "./",
  "start_url": "./?v=20250910",
  "features": [
    "image_processing",
    "color_analysis", 
    "kmeans_clustering",
    "color_mapping",
    "stringart_generation",
    "telegram_integration"
  ],
  "supported_formats": [
    "image/jpeg",
    "image/png", 
    "image/webp",
    "image/gif"
  ],
  "algorithms": {
    "clustering": [
      "k-means",
      "tones"
    ],
    "color_space": "LAB",
    "distance_metric": "Delta E"
  }
}
